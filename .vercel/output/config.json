{
  "version": 3,
  "routes": [
    {
      "source": "/api/jit/getSystemPrompt",
      "destination": "/api/system-prompt.js"
    },
    {
      "source": "/api/jit/getLivingMemory",
      "destination": "/api/living-memory.js"
    },
    {
      "source": "/api/jit/getOriginStory",
      "destination": "/api/origin-story.js"
    },
    {
      "source": "/api/jit/getUniquenessFunction",
      "destination": "/api/uniqueness-function.js"
