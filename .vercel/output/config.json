{
  "version": 3,
  "routes": [
    {
      "src": "/api/jit/getSystemPrompt",
      "dest": "/api/system-prompt.js"
    },
    {
      "src": "/api/jit/getLivingMemory",
      "dest": "/api/living-memory.js"
    },
    {
      "src": "/api/jit/getOriginStory",
      "dest": "/api/origin-story.js"
    },
    {
      "src": "/api/jit/getUniquenessFunction",
      "dest": "/api/uniqueness-function.js"
    },
    {
      "src": "/api/jit/updateMemory",
      "dest": "/api/update-memory.js"
    },
    {
      "src": "/api/jit/(?<path>.*)",
      "dest": "/api/$path.js"
    },
    {
      "src": "/api/(?<path>.*)",
      "dest": "/api/$path.js"
    }
  ]
}